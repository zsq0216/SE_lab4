<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.team.ShopSystem.sys.mapper.CartMapper">
    <select id="getCartByUserId" resultType="java.lang.Integer">
        SELECT id FROM ss_cart
        WHERE user_id = #{userId}
    </select>
</mapper>