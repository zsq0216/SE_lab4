<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.team.ShopSystem.sys.mapper.MerchantOrderMapper">
    <select id="selectByUserOrderId" resultType="com.team.ShopSystem.sys.entity.MerchantOrder">
        SELECT * FROM ss_merchant_order
        WHERE user_order_id = #{orderId}
    </select>
</mapper>